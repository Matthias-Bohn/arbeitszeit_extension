{
  "manifest_version": 2,
  "name": "Arbeitszeit Rechner",
  "version": "1.3",
  "description": "Ein Tool zur Berechnung der Arbeitszeit und Pausen.",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  },
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>"
  ],
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  }
}